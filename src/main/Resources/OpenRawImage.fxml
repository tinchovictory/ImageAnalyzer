<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ar.edu.itba.ati.GUI.Windows.OpenRawImage">
   <children>
      <Label text="Insert raw image dimensions" />
      <HBox alignment="CENTER" prefHeight="45.0" prefWidth="600.0" spacing="20.0">
         <children>
            <HBox prefHeight="35.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="70.0" text="Width" />
                  <TextField fx:id="widthField" prefHeight="27.0" prefWidth="80.0" />
               </children>
            </HBox>
            <HBox prefHeight="40.0" prefWidth="200.0" spacing="10.0">
               <children>
                  <Label alignment="CENTER" prefHeight="17.0" prefWidth="70.0" text="Height" />
                  <TextField fx:id="heightField" prefHeight="27.0" prefWidth="80.0" />
               </children>
            </HBox>
         </children>
      </HBox>
      <Button fx:id="openImage" mnemonicParsing="false" onAction="#openImageClicked" text="Open Image" />
   </children>
